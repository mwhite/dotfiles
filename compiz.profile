[extrawm]
as_toggle_fullscreen_key = <Alt>F11
as_toggle_always_on_top_key = <Control><Alt>t

[core]
as_active_plugins = core;ccp;move;resize;place;decoration;mousepoll;extrawm;imgjpeg;workarounds;png;dbus;text;grid;gnomecompat;session;svg;vpswitch;put;neg;regex;winrules;resizeinfo;commands;screenshot;obs;snap;animation;wall;fade;shelf;expo;scale;scalefilter;ezoom;scaleaddon;staticswitcher;
as_hide_skip_taskbar_windows = false
as_cursor_theme = default
as_autoraise = false
as_autoraise_delay = 500
as_maximize_window_key = Disabled
as_toggle_window_maximized_key = <Alt>F10
s0_hsize = 2
s0_vsize = 2

[rotate]
s0_zoom = 0.360300

[place]
s0_position_matches = title=trans99;
s0_position_x_values = 754;
s0_position_y_values = 414;
s0_position_constrain_workarea = false;

[scale]
s0_speed = 8.788500

[animation]
s0_minimize_durations = 200;
s0_shade_durations = 50;

[wall]
as_miniscreen = true
as_preview_timeout = 0.000000
as_preview_scale = 326
as_outline_color = #ffffff00
as_slide_duration = 0.000000
s0_edgeflip_move = true
s0_edgeflip_dnd = true

[fade]
s0_fullscreen_visual_bell = true

[decoration]
as_shadow_radius = 9.000000
as_command = gtk-window-decorator --replace
as_decoration_match = !title=trans99
as_shadow_match = any & !class=Gnome-panel

[grid]
as_put_center_key = <Control><Alt>j
as_put_left_key = <Control><Alt>h
as_put_right_key = <Control><Alt>k
as_put_top_key = <Control><Alt>u
as_put_bottom_key = <Control><Alt>m
as_put_topleft_key = <Control><Alt>y
as_put_topright_key = <Control><Alt>i
as_put_bottomleft_key = <Control><Alt>n
as_put_bottomright_key = <Control><Alt>comma

[gnomecompat]
as_run_command_terminal_key = F10

[put]
as_put_viewport_left_key = <Control><Alt><Super>Left
as_put_viewport_right_key = <Control><Alt><Super>Right
as_put_viewport_up_key = <Control><Alt><Super>Up
as_put_viewport_down_key = <Control><Alt><Super>Down
as_put_left_key = Disabled
as_put_right_key = Disabled
as_put_top_key = Disabled
as_put_bottom_key = Disabled
as_put_topleft_key = Disabled
as_put_topright_key = Disabled
as_put_bottomleft_key = Disabled
as_put_bottomright_key = Disabled
s0_unfocus_window = true
s0_speed = 50.000000

[obs]
s0_opacity_matches = title=trans99;class=Conky;class=Gnome-terminal;
s0_opacity_values = 100;80;100;

[expo]
as_zoom_time = 0.100000
as_distance = 0.069900
as_hide_docks = true
as_reflection = false

[switcher]
as_next_key = Disabled
as_prev_key = Disabled
as_next_all_key = Disabled
as_prev_all_key = Disabled
as_next_panel_key = <Control><Alt>Tab

[winrules]
s0_skiptaskbar_match = title=trans99
s0_skippager_match = title=trans99
s0_below_match = title=trans99
s0_sticky_match = class=Conky  | title=trans99
s0_no_move_match = class=Conky | title=trans99
s0_no_resize_match = class=Conky | title=trans99
s0_no_minimize_match = class=Conky | title=trans99
s0_no_maximize_match = class=Conky | title=trans99
s0_no_close_match = class=Conky | title=trans99
s0_size_matches = title=trans99;
s0_size_width_values = 256;
s0_size_height_values = 170;

[3d]
s0_max_window_space = 50
s0_width = 0.000000

[shift]
s0_mode = 1

[bench]
as_position_x = 230
as_position_y = 221

